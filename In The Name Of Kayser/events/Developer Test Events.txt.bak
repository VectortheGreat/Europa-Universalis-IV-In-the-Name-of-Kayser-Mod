country_event = {	#Reform Missions
	id = dev.6
	title = "dev.6_title"
	desc = "dev.6_desc"
	picture = ORTHODOX_eventPicture
	
	is_triggered_only = yes
	
	option = {
		name = dev6.a		
		every_country = {
					limit = {
						is_part_of_hre = yes
						NOT = { religion = ROOT }
						NOT = { total_development = 200 }
						NOT = { tag = ITR }
						NOT = { is_emperor = yes }
					}
					country_event = {
						id = itest.5
					}
				}
	}
	option = {
		name = dev6.b
		ai_chance = { factor = 25 }
		every_country = {
					limit = {
						is_part_of_hre = yes
						NOT = { religion = ROOT }
						NOT = { total_development = 200 }
						NOT = { tag = ITR }
						NOT = { is_emperor = yes }
					}
					country_event = {
						id = itest.5
					}
				}
	}
}

